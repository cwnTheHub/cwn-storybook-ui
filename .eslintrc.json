{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "plugin:storybook/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/no-unknown-property": ["error", { "ignore": ["css"] }],
    "semi": ["error", "always"],
    "quotes": ["error", "double"]
  }
}

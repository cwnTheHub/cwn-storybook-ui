{
  "name": "@cwn_ui/ui",
  "version": "1.0.0",
  "private": false,
  "dependencies": {
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "array-find": "^1.0.0",
    "array-find-es6": "^2.0.3",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.5",
    "sort-by": "^1.2.0",
    "styled-components": "^5.3.8",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build -s public",
    "semantic-release": "semantic-release",
    "lint": "eslint "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^7.0.10",
    "@storybook/addon-essentials": "^7.0.10",
    "@storybook/addon-interactions": "^7.0.10",
    "@storybook/addon-links": "^7.0.10",
    "@storybook/node-logger": "^7.0.10",
    "@storybook/preset-create-react-app": "^7.0.10",
    "@storybook/react": "^7.0.10",
    "@storybook/react-webpack5": "^7.0.10",
    "@storybook/testing-library": "^0.1.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.36.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "glob-parent": ">=5.1.2",
    "jest": "^29.5.0",
    "nth-check": ">=2.0.1",
    "object-path": ">=0.11.8",
    "prop-types": "^15.8.1",
    "semantic-release": "^20.1.3",
    "storybook": "^7.0.10",
    "trim": ">=0.0.3",
    "trim-newlines": ">=3.0.1",
    "webpack": ">=5.76.0"
  },
  "description": "cwn packages",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cwnTheHub/cwn-storybook-ui.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cwnTheHub/cwn-storybook-ui/issues"
  },
  "homepage": "https://github.com/cwnTheHub/cwn-storybook-ui#readme",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "tagFormat": "v${version}",
    "dryRun": false,
    "ci": true,
    "repositoryUrl": "https://github.com/cwnTheHub/cwn-storybook-ui.git"
  }
}
